{
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2017",
    "sourceMap": false,
    "outDir": "mem-sdk/dist",
    "baseUrl": ".",
    "incremental": true,
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "importHelpers": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "test", "lib", "**/*spec.ts"]
}